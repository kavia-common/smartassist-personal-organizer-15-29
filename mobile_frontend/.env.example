# Port configuration for development server
EXPO_PUBLIC_PORT=3000

# Trust proxy settings for reverse proxy setups
EXPO_PUBLIC_TRUST_PROXY=

# Logging level (debug, info, warn, error)
EXPO_PUBLIC_LOG_LEVEL=info

# Health check endpoint path
EXPO_PUBLIC_HEALTHCHECK_PATH=/health

# Feature flags (comma-separated list)
EXPO_PUBLIC_FEATURE_FLAGS=

# Enable experimental features (true/false)
EXPO_PUBLIC_EXPERIMENTS_ENABLED=false
